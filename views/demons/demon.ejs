<title>Demons</title>
<h1>Demons:</h1>
  <a href="/demons/new">Add New Level</a>
  <ul>
    <% demons.forEach(demonItem => { %>
      <li>
        <h2><%= demonItem.title %></h2>
        <p><%= demonItem.description %></p>
        <% if (demonItem.coverImageType && demonItem.coverImage) { %>
          <img src="data:<%= demonItem.coverImageType %>;base64,<%= demonItem.coverImage.toString('base64') %>" alt="<%= demonItem.title %> Cover Image" style="width: 200px;">
        <% } %>
      </li>
    <% }) %>
  </ul>

